    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "a",
            "outputCapture": "std",
            "program": "${workspaceFolder}/example/api-http_external-dummy/a.js",
            "runtimeArgs": ["--preserve-symlinks"],
            "args": [
                "--api.port=9001"
            ],
            "env": {"NODE_PATH": "${workspaceRoot}/dev/"}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "b",
            "outputCapture": "std",
            "program": "${workspaceFolder}/example/api-http_external-dummy/b.js",
            "runtimeArgs": ["--preserve-symlinks"],
            "args": [
                "--api.port=9002"
            ],
            "env": {"NODE_PATH": "${workspaceRoot}/dev/"}
        }
    ],
    "compounds": [{
        "name": "a+b",
        "configurations": ["a", "b"]
    }]
}